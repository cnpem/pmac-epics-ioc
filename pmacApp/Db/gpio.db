
record(calc, "$(P):GI00")
{
    field(PINI, "YES")
    field(SCAN, ".2 second")
    field(CALC, "(A & B) >> C")
    field(INPA, "BRICK1:READ_GPIO_INP NPP NMS")
    field(B, "1")                 # mask
    field(C, "0")               # shift right
}


